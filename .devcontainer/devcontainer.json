{
  "name": "Servicemap Backend",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "servicemap",
  "workspaceFolder": "/smbackend",
  "runServices": [
    "servicemap",
    "postgres",
    "redis"
  ],
  "shutdownAction": "stopCompose",
  "forwardPorts": [3000],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    },
    "jetbrains" : {
      "backend" : "IntelliJ"
    }
  }
}